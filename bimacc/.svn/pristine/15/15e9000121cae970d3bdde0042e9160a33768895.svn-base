<script>
  $('#disputecategory').change(function(){  
    var categoryID = $(this).val();     
    if(categoryID){
      $.ajax({
       type:"GET",
       url:"<?php echo e(url('/get-dispute-list')); ?>?dispute_categories_id="+categoryID,
       success:function(res){                      
        if(res){
          $("#subcategory").empty();
          $('#subcategory').prop( "disabled", false );  
          $("#subcategory").append('<option value=""> Select Sub Category</option>');
          $.each(sortReponse(res),function(key,value){
            console.log(value);
            $("#subcategory").append('<option value="'+value.key+'">'+value.value+'</option>');
          });

        }else{
         $("#subcategory").empty();
       }
     }
   });
    }     
  });
  function sortReponse(data)
{
    var sorted = [];
    $(data).each(function(k, v) {
        for(var key in v) {
            sorted.push({key: key, value: v[key]})
        }
    });

    return sorted.sort(function(a, b){
        if (a.value < b.value) return -1;
        if (a.value > b.value) return 1;
        return 0;
    });
}
</script>
