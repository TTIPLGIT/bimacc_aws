<script type="text/javascript">
   function status_select()
 {
  var inputvalue = document.getElementById("claimnoticestatus").value;

// alert (inputvalue);

  if( inputvalue==="Accepting and Admitting the Claim"){

    $("#update").show();
    $("#make_counter").hide();
    

    
  }
  else if( inputvalue==="Disputing and Contesting the Claim")
  {
   $("#update").show();
   $("#make_counter").hide();
    $("#dispute_select").hide();
  

 }
 else if( inputvalue==="Making a Counter Claim")
 {
  $("#update").hide();
  $("#make_counter").show();
  $("#dispute_select").show();
  
   

 }
 else 
 {
   $("#update").show();
   // $("#make_counter").hide();
  


 }
 



}
</script>
